LOAD Student
LOAD Advisor
LOAD Marks
ADVISOR_2 <- SELECT Advisor_id == 2 FROM Student
MATHS_C_ADVISOR <- CROSS Marks ADVISOR_2
MATHS_ADVISOR <- SELECT Marks_Stud_Id == ADVISOR_2_Stud_Id FROM MATHS_C_ADVISOR
MATHS_90 <- SELECT Maths_marks > 90 FROM MATHS_ADVISOR
RESULT_h <- PROJECT Marks_Stud_Id FROM MATHS_90
PRINT RESULT_h