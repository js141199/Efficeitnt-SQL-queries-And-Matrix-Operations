LOAD Student
LOAD Advisor
LOAD Marks
STUDENTS <- SELECT Advisor_id == 1 FROM Student
FEMALE_STUDENTS <- SELECT Stud_sex == 1 FROM STUDENTS
FEMALE_C_MARKS <- CROSS FEMALE_STUDENTS Marks
FEMALE_MARKS <- SELECT FEMALE_STUDENTS_Stud_Id == Marks_Stud_Id
FEMALE_MATHS <- SELECT Maths_marks > 70 FROM FEMALE_MARKS
RESULT_E <- PROJECT Marks_Stud_Id, Maths_marks FROM FEMALE_MATHS
PRINT RESULT_E